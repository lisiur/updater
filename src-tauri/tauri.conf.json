{
  "build": {
    "beforeDevCommand": "pnpm dev",
    "beforeBuildCommand": "pnpm build",
    "devPath": "http://localhost:1420",
    "distDir": "../dist",
    "withGlobalTauri": true
  },
  "package": {
    "productName": "updater",
    "version": "0.0.3"
  },
  "tauri": {
    "allowlist": {
      "all": false,
      "shell": {
        "all": false,
        "open": true
      }
    },
    "bundle": {
      "active": true,
      "icon": [
        "icons/32x32.png",
        "icons/128x128.png",
        "icons/128x128@2x.png",
        "icons/icon.icns",
        "icons/icon.ico"
      ],
      "identifier": "com.lisiur.dev",
      "targets": "all"
    },
    "security": {
      "csp": null
    },
    "updater": {
      "active": true,
      "endpoints": ["https://lisiur.github.io/updater/latest.json"],
      "dialog": true,
      "pubkey": "dW50cnVzdGVkIGNvbW1lbnQ6IG1pbmlzaWduIHB1YmxpYyBrZXk6IEZEMTAwM0ZCOTE5M0NBODIKUldTQ3lwT1Ird01RL2RQLzkxWlN3eFV6MG1WQm9COWpqOHcrNk9lb3NHU2VDNm1XZ0E0RzBsK2EK"
    },
    "windows": [
      {
        "fullscreen": false,
        "resizable": true,
        "title": "updater",
        "width": 800,
        "height": 600
      }
    ]
  }
}
